{
  "name": "flowbite-svelte",
  "version": "2.0.0-next.3",
  "description": "Flowbite components for Svelte",
  "author": {
    "name": "Shinichi Okada",
    "email": "connect@codewithshin.com",
    "url": "https://blog.codewithshin.com"
  },
  "bugs": "https://github.com/themesberg/flowbite-svelte/issues",
  "homepage": "https://flowbite-svelte.com/",
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "test": "npm run test:e2e && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:ignore": "npx sv check --ignore 'src/routes/testdir,dist,build' --no-tsconfig",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "check:all": "pnpm format && pnpm lint && pnpm check && pnpm test:e2e && pnpm test:unit:run",
    "lint": "pnpm format && prettier --check . && eslint .",
    "format": "prettier --write .",
    "test:e2e": "playwright test",
    "test:unit": "vitest",
    "test:unit:run": "vitest run",
    "test:unit-t": "vitest run -t",
    "gen:docspropvalue": "svelte-lib-helpers docspropvalue themesberg/flowbite-svelte",
    "gen:component-data-prop-value": "svelte-lib-helpers component-data-prop-value themesberg/flowbite-svelte",
    "copy:packagejson": "svelte-lib-helpers package",
    "sync:exports": "./scripts/sync-plugin-utilities-export.sh",
    "lib-helpers": "pnpm gen:component-data-prop-value && pnpm gen:docspropvalue && pnpm format && pnpm package && pnpm copy:packagejson && pnpm llm",
    "llm": "svelte-doc-llm && ./scripts/update-llms.sh",
    "ch": "npx changeset",
    "cv": "npx changeset version",
    "cp": "npx changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@docsearch/css": "^4.4.0",
    "@docsearch/js": "^4.4.0",
    "@eslint/compat": "^2.0.0",
    "@eslint/js": "^9.39.2",
    "@flowbite-svelte-plugins/chart": "^0.2.4",
    "@flowbite-svelte-plugins/datatable": "^0.4.1",
    "@flowbite-svelte-plugins/texteditor": "^0.25.6",
    "@playwright/test": "^1.57.0",
    "@resvg/resvg-js": "^2.6.2",
    "@sveltejs/adapter-vercel": "^6.2.0",
    "@sveltejs/kit": "^2.49.2",
    "@sveltejs/package": "^2.5.7",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@svitejs/changesets-changelog-github-compact": "^1.2.0",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/svelte": "^5.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@tiptap/core": "3.7.2",
    "@vitest/browser": "^4.0.16",
    "@vitest/browser-playwright": "^4.0.16",
    "dayjs": "^1.11.19",
    "deepmerge": "^4.3.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.13.1",
    "flowbite-svelte-admin-dashboard": "^2.1.1",
    "flowbite-svelte-blocks": "^2.1.0",
    "flowbite-svelte-icons": "^3.0.1",
    "flowbite-svelte-illustrations": "^1.0.6",
    "flowbite-typography": "^1.0.5",
    "globals": "^16.5.0",
    "highlight.js": "^11.11.1",
    "jsdom": "^27.4.0",
    "katex": "^0.16.27",
    "lowlight": "^3.3.0",
    "mdsvex": "^0.12.6",
    "mdsvexamples": "^0.5.0",
    "playwright": "^1.57.0",
    "prettier": "^3.7.4",
    "prettier-plugin-svelte": "^3.4.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "prism-themes": "^1.9.0",
    "publint": "^0.3.16",
    "runatics": "^0.2.0",
    "runes-meta-tags": "^0.5.0",
    "satori": "^0.18.3",
    "satori-html": "^0.3.2",
    "super-sitemap": "^1.0.5",
    "svelte": "^5.46.1",
    "svelte-check": "^4.3.5",
    "svelte-doc-llm": "^0.8.0",
    "svelte-lib-helpers": "^0.4.31",
    "svelte-meta-tags": "^4.5.0",
    "svelte-rune-highlight": "^0.12.1",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1",
    "vite": "^7.3.0",
    "vite-plugin-devtools-json": "^1.0.0",
    "vitest": "^4.0.16",
    "vitest-browser-svelte": "^2.0.1"
  },
  "peerDependencies": {
    "svelte": "^5.40.0",
    "tailwindcss": "^4.1.4"
  },
  "type": "module",
  "keywords": [
    "svelte",
    "sveltekit",
    "tailwindcss",
    "flowbite",
    "ui",
    "accordion",
    "alert",
    "avatar",
    "badge",
    "breadcrumb",
    "button-group",
    "button",
    "card",
    "carousel",
    "darkmode",
    "dropdown",
    "footer",
    "forms",
    "kbd",
    "list group",
    "mega menu",
    "modal",
    "navbar",
    "pagination",
    "progressbar",
    "rating",
    "sidebar",
    "skeleton",
    "spinner",
    "table",
    "tab",
    "timeline",
    "toast",
    "tooltip",
    "typography",
    "video"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/themesberg/flowbite-svelte.git"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.7.4",
    "@floating-ui/utils": "^0.2.10",
    "apexcharts": "^5.3.6",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "esm-env": "^1.2.2",
    "flowbite": "^4.0.1",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "default": "./dist/types.js"
    },
    "./package.json": "./package.json"
  }
}
