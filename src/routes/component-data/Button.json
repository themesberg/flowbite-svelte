{
  "name": "Button",
  "slots": [],
  "events": ["on:click", "on:change", "on:keydown", "on:keyup", "on:touchstart", "on:touchend", "on:touchcancel", "on:mouseenter", "on:mouseleave"],
  "props": [
    ["pill", "boolean", "false"],
    ["outline", "boolean", "false"],
    ["size", "'xs' | 'sm' | 'md' | 'lg' | 'xl'", "group ? 'sm' : 'md'"],
    ["href", "string | undefined", "undefined"],
    ["type", "ButtonType", "'button'"],
    ["color", "ButtonColor", "group ? (outline ? 'dark' : 'alternative') : 'primary'"],
    ["shadow", "boolean", "false"]
  ]
}
